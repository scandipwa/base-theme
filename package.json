{
  "name": "scandipwa-base",
  "version": "1.0.0",
  "private": true,
  "author": "Scandiweb info@scandiweb.com",
  "license": "OSL-3.0",
  "proxy": "https://de-demo.scandipwa.com/",
  "engineStrict": true,
  "engines": {
    "node": "^10"
  },
  "scripts": {
    "prepare": "node ./src/config/Extensibility/extensions-dependency-installer.js",
    "watch-core": "webpack-dev-server --env.BUILD_MODE=core --config ./src/config/webpack.development.config.js",
    "watch": "webpack-dev-server --config ./src/config/webpack.development.config.js",
    "watch-sw": "webpack --config ./src/config/webpack.sw.config.js --mode=development --watch",
    "build": "npm run build-sw && webpack --config ./src/config/webpack.production.config.js",
    "build-sw": "webpack --config ./src/config/webpack.sw.config.js --mode=production",
    "test": "jest --env=jsdom --watchAll",
    "eslint": "eslint ./src/app --ignore-pattern *.test.*",
    "eslint:fix": "npm run eslint -- --fix",
    "eslint:cache": "eslint ./src/app --cache --ignore-pattern *.test.*",
    "dev-server": "forever ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.BUILD_MODE=development --config ./src/config/webpack.development.config.js  --progress",
    "dev-server-core": "forever ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.BUILD_MODE=core --config ./src/config/webpack.development.config.js  --progress",
    "watch-dev-server": "SHELL=/bin/bash chokidar --silent \"src/**/*.js\" \"src/**/*.scss\"  -c \"[ '{event}' = 'add' ] || [ '{event}' = 'unlink' ] && forever restartall \"",
    "extract-translations": "webpack --config ./src/config/webpack.extract-translations.config.js --progress --hide-module",
    "pm2-watch": "./node_modules/.bin/pm2 start process.yml --no-daemon",
    "stylelint": "stylelint ./src/**/*.scss",
    "stylelint:fix": "npm run stylelint -- --fix",
    "stylelint:cache": "stylelint --cache ./src/**/*.scss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run eslint:cache && npm run stylelint:cache"
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/src/config/tests.config.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/EmptyModule",
      "\\.(css|scss|style)$": "<rootDir>/EmptyModule"
    }
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "braintree-web-drop-in": "^1.20.4",
    "html-react-parser": "^0.6.1",
    "ignore-loader": "^0.1.2",
    "loader-utils": "^1.4.0",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-debounce-render": "^5.0.0",
    "react-dom": "^16.6.3",
    "react-input-range": "^1.3.0",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-vimeo": "^2.0.0",
    "react-visibility-sensor": "^5.1.1",
    "react-youtube": "^7.9.0",
    "react-zoom-pan-pinch": "^1.5.5",
    "rebem-classname": "^0.4.0",
    "redux": "^4.0.1",
    "unstated": "^2.1.1"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-proposal-optional-chaining": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-arrow-functions": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.0",
    "@babel/preset-react": "^7.10.4",
    "@babel/runtime": "^7.11.2",
    "@scandipwa/eslint-plugin-scandipwa-guidelines": "^1.10.1",
    "autoprefixer": "^9.8.6",
    "babel-cli": "^6.26.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-loader": "^8.0.4",
    "babel-minify-webpack-plugin": "^0.3.1",
    "babel-plugin-console-source": "^2.0.1",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-property-arrow-functions": "^1.0.1",
    "babel-plugin-transform-rebem-jsx": "^0.3.3",
    "babel-plugin-transform-replace-object-assign": "^2.0.0",
    "babel-preset-airbnb": "^3.2.0",
    "chokidar-cli": "^1.2.2",
    "clean-webpack-plugin": "^1.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-hot-loader": "^1.4.3",
    "css-loader": "^4.3.0",
    "cssnano": "^4.1.10",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-array-func": "^3.1.7",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^24.0.2",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "file-loader": "^2.0.0",
    "forever": "^1.0.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.3.0",
    "jest": "^23.6.0",
    "mini-css-extract-plugin": "^0.11.2",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "pm2": "^3.5.1",
    "postcss-cli": "^6.1.0",
    "postcss-loader": "^3.0.0",
    "redux-devtools": "^3.4.2",
    "sass-loader": "^10.0.2",
    "sass-resources-loader": "^2.1.0",
    "style-loader": "^1.2.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.18.0",
    "terser": "^5.1.0",
    "terser-webpack-plugin": "^3.1.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-pwa-manifest": "^4.2.0",
    "workbox-webpack-plugin": "^3.6.3"
  }
}
